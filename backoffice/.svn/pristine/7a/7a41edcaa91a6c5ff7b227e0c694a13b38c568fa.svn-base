<div class="card">
  <div class="card-header">
    <i class="fa fa-align-justify"></i> {{title}}
  </div>
  <div class="card-block">
    <form class="form-inline" (ngSubmit)="search()">
      <div class="form-group">
        <label>{{ 'common.date' | translate }}：</label>
        <p-calendar  hourFormat="24"  [(ngModel)]="startTime"  class="ndate"  dateFormat="yy-mm-dd" [showTime]="true"
                     [ngModelOptions]="{standalone: true}" [monthNavigator]="true" [yearNavigator]="true"
                     yearRange="2000:2030" required></p-calendar>
      </div>
      <div class="form-group">
        <label>{{ 'common.to' | translate }}</label>
        <p-calendar  hourFormat="24"  [(ngModel)]="lastTime"   class="ndate" dateFormat="yy-mm-dd" [showTime]="true"
                     [ngModelOptions]="{standalone: true}" [monthNavigator]="true" [yearNavigator]="true"
                     yearRange="2000:2030" required></p-calendar>
      </div>
     <!-- <div class="form-group">
        <label for="searchUserName">{{ 'common.userName' | translate }}</label>
        <input  id="searchUserName" name="searchUserName"  [(ngModel)]="uName"  type="text" class="form-control input-xlarge"  [placeholder]="'common.userName' | translate" [ngModelOptions]="{standalone: true}">
      </div>-->
      <div class="btn-group">
        <button type="submit" class="margin-left9 btn btn-primary">{{ 'common.search' | translate }}</button>
      </div>
    </form>
    <!-- 输赢展示面板-->
    <div #winLosePanel>
    </div>
  </div>
</div>

